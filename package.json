{
    "name": "googlecloudstorage",
    "label": "Google Cloud Storage",
    "dependencies": [
        {
            "type": "service",
            "name": "http",
            "version": "v1.7.0",
            "instanceId": "shared"
        }
    ],
    "configuration": [
        {
            "name": "serviceAccountEmail",
            "label": "Service Account Email",
            "description": "The email created for the service account",
            "type": "text",
            "required": true
        },
        {
            "name": "privateKey",
            "label": "Private Key",
            "description": "The private key created for the service account",
            "type": "password",
            "required": true
        },
        {
            "name": "GOOGLE_STORAGE_API_BASE_URL",
            "label": "Google Storage Api Url",
            "type": "label",
            "value": "https://storage.googleapis.com"
        }
    ],
    "events": [],
    "metadata": [
        {
            "type": "script",
            "namespace": "api",
            "path": "/scripts/api.js"
        },
        {
            "type": "script",
            "namespace": "buckets",
            "path": "/scripts/buckets.js"
        },
        {
            "type": "script",
            "namespace": "objects",
            "path": "/scripts/objects.js"
        },
        {
            "type": "script",
            "namespace": "utils",
            "path": "/scripts/utils.js"
        }
    ]
}
